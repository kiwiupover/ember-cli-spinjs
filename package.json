{
  "name": "ember-cli-spinjs",
  "version": "2.0.0",
  "description": "An ember addon to integrate with spin.js",
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://ember-cli-spinjs.surge.sh"
  },
  "keywords": [
    "ember-addon",
    "ember-cli",
    "spinjs",
    "ember-cli-spinjs",
    "blockchain"
  ],
  "repository": "https://github.com/kiwiupover/ember-cli-spinjs",
  "license": "MIT",
  "author": "David Laird",
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember server",
    "test": "ember test",
    "test:all": "ember try:each",
    "deploy": "ember surge",
    "publish:patch": "npm whoami && npm version patch && git push origin --tags && npm publish",
    "publish:minor": "npm whoami && npm version minor && git push origin --tags && npm publish"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.7.0",
    "bower": "^1.8.2",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.10.1",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-htmlbars": "^4.0.0",
    "ember-cli-htmlbars-inline-precompile": "^2.0.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-surge": "^1.2.0",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-test-loader": "^2.1.0",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.0.0",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.5.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.1.0",
    "eslint-plugin-ember": "^7.0.0",
    "eslint-plugin-node": "^8.0.0",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.8.0"
  },
  "dependencies": {
    "broccoli-funnel": "^3.0.3",
    "broccoli-merge-trees": "^3.0.1",
    "ember-auto-import": "^1.2.15",
    "ember-cli-babel": "^7.1.2",
    "resolve": "^1.8.1",
    "spin.js": "^4.0.0"
  },
  "bugs": {
    "url": "https://github.com/kiwiupover/ember-cli-spinjs/issues"
  },
  "homepage": "https://github.com/kiwiupover/ember-cli-spinjs"
}
