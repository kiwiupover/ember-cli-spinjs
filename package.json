{
  "name": "ember-cli-spinjs",
  "version": "1.3.0",
  "description": "An ember addon to integrate with spin.js",
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://ember-cli-spinjs.surge.sh",
    "versionCompatibility": {
      "ember": "~1.13.0 || ~2.4.0 || > 2.8.0"
    }
  },
  "keywords": [
    "ember-addon",
    "ember-cli",
    "spinjs",
    "ember-cli-spinjs"
  ],
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "start": "ember server",
    "test": "ember try:each",
    "deploy": "ember surge",
    "publish:patch": "npm whoami && npm version patch && git push origin --tags && npm publish",
    "publish:minor": "npm whoami && npm version minor && git push origin --tags && npm publish"
  },
  "repository": "https://github.com/kiwiupover/ember-cli-spinjs",
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "author": "David Laird",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.5.0",
    "ember-cli": "~2.18.0",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.1.0",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.0.0",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-surge": "1.2.0-beta.2",
    "ember-cli-test-loader": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-cli-update": "^0.14.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.1.0",
    "ember-source": "~2.18.0",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^6.0.0",
    "loader.js": "^4.2.3",
    "bower": "^1.8.2"
  },
  "dependencies": {
    "ember-cli-babel": "^6.6.0",
    "ember-cli-es6-transform": "^0.0.3",
    "spin.js": "^3.0.0"
  },
  "bugs": {
    "url": "https://github.com/kiwiupover/ember-cli-spinjs/issues"
  },
  "homepage": "https://github.com/kiwiupover/ember-cli-spinjs"
}