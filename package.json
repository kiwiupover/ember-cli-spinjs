{
  "name": "ember-cli-spinjs",
  "version": "1.0.7",
  "description": "An ember-cli addon to integrate with spin.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://ember-cli-spinjs.surge.sh",
    "versionCompatibility": {
      "ember": ">=1.10.0"
    }
  },
  "keywords": [
    "ember-addon",
    "ember-cli",
    "spinjs",
    "ember-cli-spinjs"
  ],
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember try:testall"
  },
  "repository": "https://github.com/kiwiupover/ember-cli-spinjs",
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "David Laird",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.2.0",
    "ember-cli": "2.7.0",
    "ember-cli-app-version": "^2.0.0",
    "ember-cli-content-security-policy": "0.5.0",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-htmlbars": "^1.0.1",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-inject-live-reload": "^1.3.1",
    "ember-cli-qunit": "^3.0.0",
    "ember-cli-release": "0.2.8",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-surge": "1.1.8",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-disable-proxy-controllers": "^1.0.1",
    "ember-export-application-global": "^1.0.4",
    "ember-load-initializers": "^0.5.0",
    "ember-resolver": "^2.0.3",
    "ember-try": "~0.2.2",
    "ember-watson": "^0.8.2",
    "loader.js": "^4.0.0"
  },
  "dependencies": {
    "ember-cli-babel": "^5.1.5"
  },
  "bugs": {
    "url": "https://github.com/kiwiupover/ember-cli-spinjs/issues"
  },
  "homepage": "https://github.com/kiwiupover/ember-cli-spinjs"
